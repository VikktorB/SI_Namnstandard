      <Object type="Group">
        <Name>Internal</Name>
        <Object type="EXOLVariables">
          <Name>Variables</Name>
          <Attribute name="Variables"> 
X OldHour
L Exercise 
I Counter
L ExerciseSecondPart</Attribute>
          <Attribute name="Top">10</Attribute>
          <Attribute name="Left">245</Attribute>
        </Object>
        <Object type="EXOLCode">
          <Name>Code</Name>
          <Attribute name="ObjectChanged">Modified</Attribute>
          <Attribute name="Code">If Enable
  ; Time for Exercise?
  If (CmpX(Wday = ExWeekday) Or CmpX(ExWeekDay = 8)) And Not StartUpFlag Or ExCmd
     If CmpX(Hour = ExHour) And CmpX(Hour &lt;&gt; OldHour) And Not Output Or ExCmd And Not Output
        ; yes
        If CmpX(ExTime &gt; 59)
          X ExTime = 59
        EndIf
        If ExCmd
		  L ExCmd = False!
        EndIf
        L Exercise = Not Blocked    ;1
     EndIf
  EndIf
  X OldHour = Hour
 
  If Exercise OR ExerciseSecondPart 
	  If CmpI(Counter &lt; CvX(ExTime)*60)
		I Counter = Counter + CvX(SecDiff)
	  Else
	    I Counter = 0
		If Exercise AND NOT Running AND NOT ExerciseSecondPart And Not Bypass
		  L AlaExercise = True!
		  L ExerciseSecondPart = OnOffCycle  
		  L Exercise = False!
		Else
		  If Not ExerciseSecondPart And OnOffCycle
			 L ExerciseSecondPart = True!
			 L Exercise = False!
		  Else
			 If Running
				L AlaExercise = False!
			 Else
				L AlaExercise = True!
			 EndIf
			 L ExerciseSecondPart = False!
			 L Exercise = False!
		  EndIf
		EndIf
	  EndIf
  EndIf
  
  If Bypass AND NOT Blocked AND NOT ForcedON
	L Output = Input
  Else
    L Output = ((Input AND NOT ExerciseSecondPart) Or Exercise) AND NOT Blocked OR ForcedON AND NOT Blocked
  EndIf
  L ExerciseActive = Exercise Or ExerciseSecondPart
  
  If (Output AND Running OR NOT Output AND NOT Running) AND NOT Exercise AND NOT ExerciseSecondPart
	L AlaExercise = False!
  EndIf
  
Else
  L Output = False!
  I Counter = 0
  L AlaExercise = False!
EndIf
</Attribute>
          <Attribute name="Top">10</Attribute>
          <Attribute name="Left">365</Attribute>
        </Object>
      </Object>
    </Object>
