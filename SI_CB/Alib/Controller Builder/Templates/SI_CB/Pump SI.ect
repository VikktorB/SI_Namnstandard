          <Name>ResetPointPrevYearStarts</Name>
          <Attribute name="ToolTitle">Reset Point Prev. Year Starts</Attribute>
          <Attribute name="Description">The point where Prev. Year Starts will start counting from, after reset.</Attribute>
          <Attribute name="SignalType">Parameter</Attribute>
          <Attribute name="DataType">R</Attribute>
          <Attribute name="Default">0</Attribute>
          <Attribute name="Top">660</Attribute>
          <Attribute name="Left">10</Attribute>
          <Attribute name="Width">120</Attribute>
        </Object>
      </Object>
      <Object type="Group">
        <Name>Internal</Name>
        <Object type="EXOLVariables">
          <Name>Variables</Name>
          <Attribute name="Variables">  X OldDay      ; Internal. For detecting day-shift
  I IntSec      ; Internal Sec-counter
  X OldYear     ; Internal for storing year values.		
  X OldMonth    ; Internal for storing month values.	
  X OldMinute   ; Internal for storing minute values.	</Attribute>
          <Attribute name="Top">10</Attribute>
          <Attribute name="Left">325</Attribute>
        </Object>
        <Object type="EXOLCode">
          <Name>Code</Name>
          <Attribute name="ObjectChanged">Modified</Attribute>
          <Attribute name="Code">If Enable
  If StartUpFlag
        ; Avoid shift at first startup
        X OldDay    = WDay     
        X OldMonth  = Month
        X OldYear   = Year
        X OldMinute = Minute
  EndIf
  
  If CmpX( OldDay &lt;&gt; WDay )
    ; if change of day (0:00)...
    X OldDay = WDay
    
    ;Shift day runtime counters
    R TwoDaysAgoHours       = PrevDayHours
    R TwoDaysAgoMinutes     = PrevDayMinutes
    R PrevDayHours          = ThisDayHours
    R PrevDayMinutes        = ThisDayMinutes
    R ThisDayHours          = 0
    R ThisDayMinutes        = 0
    
    ;Shift day start counters
    R TwoDaysAgoStarts      = PrevDayStarts
    R PrevDayStarts         = ThisDayStarts
    R ThisDayStarts         = 0
    
    If CmpX(WDay = 1) ;New week
        ;Shift week runtime counters
        R PrevWeekHours    = ThisWeekHours
        R PrevWeekMinutes  = ThisWeekMinutes
        R ThisWeekHours    = 0
        R ThisWeekMinutes  = 0
        
        ;Shift week start counters
        R PrevWeekStarts   = ThisWeekStarts
        R ThisWeekStarts   = 0
    EndIf
  EndIf
  
  If CmpX( OldMonth &lt;&gt; Month )
    ;New month
    X OldMonth = Month
    
    ;Shift month runtime counters
    R PrevMonthHours       = ThisMonthHours
    R PrevMonthMinutes     = ThisMonthMinutes
    R ThisMonthHours       = 0
    R ThisMonthMinutes     = 0
    
    ;Shift month start counters
    R PrevMonthStarts      = ThisMonthStarts
    R ThisMonthStarts      = 0
  EndIf
  
  If CmpX( OldYear &lt;&gt; Year )
     ; if change of year...
     X OldYear = Year
     
     ;Shift year runtime counters
     R PrevYearHours    = ThisYearHours
     R PrevYearMinutes  = ThisYearMinutes
     R ThisYearHours    = 0
     R ThisYearMinutes  = 0
     
     ;Shift year start counters
     R PrevYearStarts   = ThisYearStarts
     R ThisYearStarts   = 0
  EndIf
  
  If Reset
    L Reset = False!
    R TotalHours             = CvH(CvR(ResetPointTotalHours))      ; Reset TotalHours runtime
    R TotalMinutes           = 0 
    R ThisDayHours           = CvH(CvR(ResetPointDayHours))        ; Reset Runtime today  
    R ThisDayMinutes         = 0  
    R PrevDayHours           = CvH(CvR(ResetPointYesterdayHours))  ; Reset Runtime Yesterday  
    R PrevDayMinutes         = 0
    R TwoDaysAgoHours        = CvH(CvR(ResetPointTwoDaysAgoHours)) ; Reset Runtime Two Days Ago 
    R TwoDaysAgoMinutes      = 0
    R ThisWeekHours          = CvH(CvR(ResetPointThisWeekHours))   ; Reset previous weeks' runtime
    R ThisWeekMinutes        = 0 
    R PrevWeekHours          = CvH(CvR(ResetPointPrevWeekHours))   ; Reset previous weeks' runtime
    R PrevWeekMinutes        = 0 
    R ThisMonthHours         = CvH(CvR(ResetPointThisMonthHours))  ; Reset previous months' runtime
    R ThisMonthMinutes       = 0 
    R PrevMonthHours         = CvH(CvR(ResetPointPrevMonthHours))  ; Reset previous months' runtime
    R PrevMonthMinutes       = 0 
    R ThisYearHours          = CvH(CvR(ResetPointThisYearHours))   ; Reset this years' runtime
    R ThisYearMinutes        = 0
    R PrevYearHours          = CvH(CvR(ResetPointPrevYearHours))   ; Reset previous years' runtime
    R PrevYearMinutes        = 0 
    
    R TotalStarts            = ResetPointTotalStarts	  ; TotalHours number of starts
    R ThisDayStarts          = ResetPointThisDayStarts		  ; Number of starts today  
    R PrevDayStarts          = ResetPointPrevDayStarts  ; Number of starts Yesterday
    R TwoDaysAgoStarts       = ResetPointTwoDaysAgoStarts ; Number of starts Yesterday
    R ThisWeekStarts         = ResetPointThisWeekStarts   ; Reset this weeks' no of starts
    R PrevWeekStarts         = ResetPointPrevWeekStarts   ; Reset previous weeks' no of starts
    R ThisMonthStarts        = ResetPointThisMonthStarts  ; Reset this months' no of starts
    R PrevMonthStarts        = ResetPointPrevMonthStarts  ; Reset previous months' no of starts                                                                      
    R ThisYearStarts         = ResetPointThisYearStarts   ; Reset this years' no of starts
    R PrevYearStarts         = ResetPointPrevYearStarts   ; Reset previous years' no of starts
    
   ; I IntSec = 0     ; Reset internal seccounter
  EndIf

  If Input
      ; if runtime indication
      ; Count number of starts...
      If EdgeL( Input )
        R TotalStarts       = TotalStarts + 1
        R ThisDayStarts     = ThisDayStarts + 1
        R ThisWeekStarts    = ThisWeekStarts + 1
        R ThisMonthStarts   = ThisMonthStarts + 1
        R ThisYearStarts    = ThisYearStarts + 1
      Else
        ; Count runtime...
        ;I IntSec    = IntSec + CvX(SecDiff)      
        If CmpR(TotalMinutes &lt; 59) AND CmpX(OldMinute &lt;&gt; Minute)
            X OldMinute = Minute
            ;Count up minutes
            R TotalMinutes         = TotalMinutes + 1
            R MaintenanceMinutes   = MaintenanceMinutes + 1
            R ThisDayMinutes       = ThisDayMinutes + 1
            R ThisWeekMinutes      = ThisWeekMinutes + 1
            R ThisMonthMinutes     = ThisMonthMinutes + 1
            R ThisYearMinutes      = ThisYearMinutes + 1 
        Else
            If CmpR(TotalMinutes &gt;= 59) AND CmpX(OldMinute &lt;&gt; Minute)     ;New hour
                ;Reset minute counters
                R TotalMinutes         = 0
                R MaintenanceMinutes   = 0
                R ThisDayMinutes       = 0
                R ThisWeekMinutes      = 0
                R ThisMonthMinutes     = 0
                R ThisYearMinutes      = 0
                
                ;Count up hour counters
                R TotalHours           = TotalHours + 1
                R MaintenanceHours     = MaintenanceHours + 1
                R ThisDayHours         = ThisDayHours + 1
                R ThisWeekHours        = ThisWeekHours +1
                R ThisMonthHours       = ThisMonthHours + 1
                R ThisYearHours        = ThisYearHours + 1
                ;I IntSec            = 0 ;Reset second counter
            EndIf
        EndIf
        
      EndIf     
  EndIf  ; if running

  If Service
    ; reset the serviceruntime
    L Service = False!
    R MaintenanceHours = 0
    R MaintenanceMinutes = 0
  EndIf
  L AlaMaintenance = CmpR( MaintenanceHours &gt; ServicePeriod ) AND CmpR( ServicePeriod &gt; 0 ) 
EndIf		</Attribute>
          <Attribute name="Top">10</Attribute>
          <Attribute name="Left">430</Attribute>
        </Object>
      </Object>
      <Object type="Group">
        <Name>Alarms</Name>
        <Object type="AlarmPoint">
