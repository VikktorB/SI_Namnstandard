      <Object type="SignalGroup">
        <Name>Configuration</Name>
        <Object type="Signal">
          <Name>OnOffCycle</Name>
          <Attribute name="ToolTitle">On-Off Cycle</Attribute>
          <Attribute name="Description">If this parameter is set, the exercise first forces the Output to the On state for the time set in ExTime, then forces it to the Off state for the same time.</Attribute>
          <Attribute name="SignalType">Parameter</Attribute>
          <Attribute name="DataType">L</Attribute>
          <Attribute name="Default">Yes[1]</Attribute>
          <Attribute name="Suggests">No [0], Yes[1]</Attribute>
